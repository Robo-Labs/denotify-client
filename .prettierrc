{
	"printWidth": 80,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"useTabs": true,
	"endOfLine": "auto",
	"singleAttributePerLine": false,
	"bracketSameLine": false,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"semi": false
}
